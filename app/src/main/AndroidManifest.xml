<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fgardila.ejercicio">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ejercicio">

        <activity
            android:name=".LoginActivity"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PerfilActivity"
            android:label="Perfil"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".HistorialActivity"
            android:label="Historial"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".ProductoActivty"
            android:label="Producto"
            android:parentActivityName=".ListProductosActivity" />

        <activity
            android:name=".ListProductosActivity"
            android:label="Lista de Productos"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".MainActivity"
            android:label="Inicio"/>

    </application>

</manifest>